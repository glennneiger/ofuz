<?xml version="1.0"?>
<registry>
  <rfield name="send_email">
    <rdata type="default">True</rdata>
    <rdata type="checked_value">True</rdata>
    <rdata type="hidden">0</rdata>
    <rdata type="label">Send welcome email?</rdata>
    <rdata type="checkbox">1</rdata>
    <rdata type="fieldtype">strFBFieldTypeCheckBox</rdata>
  </rfield>
  <rfield name="thankyou_page">
    <rdata type="label">Thankyou Page</rdata>
    <rdata type="fieldtype">PageBuilderFilesListBox</rdata>
  </rfield>
  <rfield name="Users_Class">
    <rdata type="default">User</rdata>
    <rdata type="textline">10:80</rdata>
    <rdata type="label">User Class</rdata>
    <rdata type="fieldtype">strFBFieldTypeChar</rdata>
  </rfield>
  <rfield name="emailtemplate">
    <rdata type="label">Welcome Email</rdata>
    <rdata type="list">emailtemplate:name:name:0:</rdata>
    <rdata type="fieldtype">strFBFieldTypeListBox</rdata>
  </rfield>
  <rfield name="admin_emailtemplate">
    <rdata type="label">Admin email alert</rdata>
    <rdata type="list">emailtemplate:name:name:0:</rdata>
    <rdata type="fieldtype">strFBFieldTypeListBox</rdata>
  </rfield>
  <rfield name="admin_email">
    <rdata type="fieldtype">strFBFieldTypeEmail</rdata>
    <rdata type="default"></rdata>
    <rdata type="email">1</rdata>
    <rdata type="label">Admin Email</rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">20:100</rdata>
  </rfield>
</registry>