<?xml version="1.0"?>
<registry>
  <rfield name="idmessage">
    <rdata type="label">idmessage</rdata>
    <rdata type="readonly">1</rdata>
    <rdata type="fieldtype">strFBFieldTypeInt</rdata>
  </rfield>
  <rfield name="key_name">
    <rdata type="default"></rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">Key name</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">:</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
    <rdata type="fieldtype">FieldTypeChar</rdata>
	<rdata type="databasetype">varchar</rdata>
  </rfield>
  <rfield name="content">
    <rdata type="default">test</rdata>
    <rdata type="html"></rdata>
    <rdata type="htmleditor"></rdata>
    <rdata type="label">Message content</rdata>
    <rdata type="required"></rdata>
    <rdata type="textarea">50:20:</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
    <rdata type="fieldtype">strFBFieldTypeText</rdata>
  </rfield>
  <rfield name="language">
    <rdata type="fieldtype">FieldTypeChar</rdata>
    <rdata type="default"></rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">Language</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">:</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
	<rdata type="databasetype">varchar</rdata>
  </rfield>
    <rfield name="context">
    <rdata type="fieldtype">FieldTypeChar</rdata>
    <rdata type="default"></rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">Context</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">20:40</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
	<rdata type="databasetype">varchar</rdata>
  </rfield>
<rfield name="plan">
    <rdata type="fieldtype">FieldTypeChar</rdata>
    <rdata type="default">all</rdata>
    <rdata type="textline">5:20</rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">Plan</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
	<rdata type="databasetype">varchar</rdata>
  </rfield>  
      <rfield name="close_duration">
    <rdata type="fieldtype">strFBFieldTypeChar</rdata>
    <rdata type="default"></rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">Closed until</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">10:20</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
	<rdata type="databasetype">varchar</rdata>
  </rfield>
  <rfield name="can_close">
    <rdata type="checked_value">yes</rdata>
    <rdata type="unchecked_value">no</rdata>
    <rdata type="default">Yes</rdata>
    <rdata type="label">Can be closed:</rdata>
    <rdata type="fieldtype">strFBFieldTypeCheckBox</rdata>
    <rdata type="checkbox">1</rdata>
    <rdata type="databasetype">varchar</rdata>
  </rfield>
</registry>
