    <rdata type="databasetype">text</rdata>
    <rdata type="label">Message</rdata>
    <rdata type="bold">Yes</rdata>
    <rdata type="italic">Yes</rdata>
    <rdata type="underline">Yes</rdata>
    <rdata type="strikethrough">Yes</rdata>
