<?xml version="1.0"?>
<registry>
  <rfield name="idtag">
    <rdata type="label">idtag</rdata>
    <rdata type="readonly">1</rdata>
    <rdata type="fieldtype">strFBFieldTypeInt</rdata>
  </rfield>
  <rfield name="tag_name">
    <rdata type="default"></rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">Tag Name</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">20:200</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
    <rdata type="fieldtype">strFBFieldTypeChar</rdata>
  </rfield>
  <rfield name="iduser">
    <rdata type="default">[do_User->iduser]</rdata>
    <rdata type="hidden"></rdata>
    <rdata type="label">iduser</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">10:20</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
    <rdata type="fieldtype">strFBFieldTypeInt</rdata>
  </rfield>
  <rfield name="reference_type">
    <rdata type="default">contact</rdata>
    <rdata type="hidden"></rdata>
    <rdata type="html"></rdata>
    <rdata type="label">associated table or object</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">30:50</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
    <rdata type="fieldtype">strFBFieldTypeChar</rdata>
  </rfield>
  <rfield name="idreference">
    <rdata type="default"></rdata>
    <rdata type="hidden"></rdata>
    <rdata type="label">primarykey the tag is attached to</rdata>
    <rdata type="readonly"></rdata>
    <rdata type="required"></rdata>
    <rdata type="textline">:</rdata>
    <rdata type="css_form_class"></rdata>
    <rdata type="css_disp_class"></rdata>
    <rdata type="css_form_style"></rdata>
    <rdata type="css_disp_style"></rdata>
    <rdata type="id"></rdata>
    <rdata type="fieldtype">strFBFieldTypeInt</rdata>
  </rfield>
  <rfield name="date_added">
    <rdata type="fieldtype">strFBFieldTypeDateSQL</rdata>
    <rdata type="datesql">Y/m/d:today:1</rdata>
    <rdata type="databasetype">date</rdata>
    <rdata type="label">Added date</rdata>
  </rfield>
</registry>